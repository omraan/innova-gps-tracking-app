		"@rneui/base": "^0.0.0-edge.2",
		"@rneui/themed": "^0.0.0-edge.2",
		"expo": "~49.0.13",
		"expo-status-bar": "~1.6.0",
		"graphql": "^15.8.0",
		"react": "18.2.0",
		"react-native": "0.72.10",
		"react-native-paper": "^5.10.6",
		"react-native-safe-area-context": "4.6.3",
